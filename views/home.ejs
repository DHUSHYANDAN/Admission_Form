    <script>
        const bgImages = ["/clg.jpeg", "/clg3.jpg", "/entrance clg.jpg"];
        function changeBackground() {
            let index = 0;
            const courseCall = document.getElementById("course_call");

            setInterval(() => {
                courseCall.style.transition = "background-image 1s ease-out";
                courseCall.style.backgroundImage = `url('${bgImages[index]}')`;
                index = (index + 1) % bgImages.length;
            }, 4000); // Change image every 5 seconds
        }
        changeBackground();
    </script>
